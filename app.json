{
  "expo": {
    "name": "apptienda",
    "slug": "apptienda",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.paframar.apptienda"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "permissions": {
      "camera": {
        "access": [
          "granted"
        ]
      }
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "534254e4-9a8d-489d-97ca-88d73c52308b"
      }
    }
  }
}
